						<ProtectedString name="Source"><![CDATA[local common = require(script.Parent.Parent.Parent.ToolsCommon)

local EXTENTS = 1000

type InfilCell = {
	Name: string,
	Floors: {BasePart},
	Roofs: {BasePart},
	Model: Model,
}

local function DecomposeCell(cell: Model) : InfilCell
	local cellModel = Instance.new("Model")
	
	local floors = {}
	local roofs = {}
	
	local isFloor = false
	for _, p in pairs(cell:GetChildren()) do
		if p.Name == "Floor" then
			isFloor = true
		elseif p.Name == "Roof" then
			isFloor = false
		else
			warn(`Ignoring Part {p.Name} in cell {cell.Name}...`)
			continue
		end
		
		local p2 = p:Clone()
		
		local t = if isFloor then floors else roofs
		local y = if isFloor then 1 else -1
		p2.Position += Vector3.yAxis*y*(EXTENTS/2)
		p2.Size += p2.CFrame:VectorToObjectSpace(Vector3.yAxis*EXTENTS)
		p2.Parent = cellModel
		t[#t+1] = p2
	end
	
	return {Name = cell.Name, Floors = floors, Roofs = roofs, Model = cellModel}
end

local function DecomposedSetParent(cell: InfilCell, parent: Instance)
	cell.Model.Parent = parent
end

local function PointInCellPart(cellPart: BasePart, point: Vector3) : boolean
	local pointLocal = cellPart.CFrame:PointToObjectSpace(point)
	--local localDistance = point - cellPart.Size
	local x = math.abs(pointLocal.X)
	local y = math.abs(pointLocal.Y)
	local z = math.abs(pointLocal.Z)
	local halfArea = cellPart.Size / 2
	return (x < halfArea.X and y < halfArea.Y and z < halfArea.Z)
end

local function PointInCell(cell: InfilCell, point: Vector3) : boolean
	for _, floor in ipairs(cell.Floors) do
		if not PointInCellPart(floor, point) then continue end
		for _, roof in ipairs(cell.Roofs) do
			if PointInCellPart(roof, point) then return true end
		end
	end
	
	return false
end

local function PartInCell(cell: InfilCell, part: BasePart) : boolean
	if PointInCell(cell, part.Position) then
		print(`Part {part.Name} in cell!`)
		return true
	end
	
	-- Check corner points
	local halfSize = part.CFrame:VectorToWorldSpace(part.Size) / 2
	for x=-1, 1, 1 do
		for y = -1, 1, 1 do
			for z = -1, 1, 1 do
				local sVec = Vector3.new(x, y, z)
				if PointInCell( cell, part.Position + (halfSize*sVec) ) then
					print(`Part {part.Name} in cell!`)
					return true
				end
			end
		end
	end
	
	return false
end

local function GetCellParts(missionGeo: Folder, cell: InfilCell) : {Instance}
	local parts = {}
	
	for _, p in missionGeo:GetDescendants() do
		if not (p:IsA("Part") or p:IsA("WedgePart") or p:IsA("CornerWedgePart")) then continue end
		print(`Checking part {p.Name} is in cell...`)
		if not PartInCell(cell, p) then continue end
		parts[#parts + 1] = p
	end
	
	print(`Part Count: {#parts}`)
	
	return parts
end

local function SelectCellParts()
	
	local debugMission = common:LevelFolder() 
	if not debugMission then return end
	
	local geo = debugMission:FindFirstChild("Geometry")
	if not geo then warn("No Geometry folder found in DebugMission!") return end
	
	local recording = common:RecordChanges("Select Cell Parts")
	if not recording then return end
	
	local tmpFolder = Instance.new("Folder")
	tmpFolder.Parent = workspace
	tmpFolder.Name = "__temp"
	
	local selecting = {}
	for _, selected in ipairs(common:GetSelection()) do
		if not selected:IsA("Model") then warn(`Can't get cell parts for Instance of type {selected.ClassName}!`) continue end
		local cell = DecomposeCell(selected)
		DecomposedSetParent(cell, tmpFolder)
		common:TableMerge(selecting, GetCellParts(geo, cell))
	end
	
	tmpFolder:Destroy()
	
	game:GetService("Selection"):Set(selecting)
	
	common:CommitChanges(recording)
	
end

return {
