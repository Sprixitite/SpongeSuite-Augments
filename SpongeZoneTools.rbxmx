	bisectLocalX.Triggered:Connect(CreateBisector(Enum.Axis.X, false))
	bisectLocalY.Triggered:Connect(CreateBisector(Enum.Axis.Y, false))
	bisectLocalZ.Triggered:Connect(CreateBisector(Enum.Axis.Z, false))

	local bisectGlobalX = common:CreatePluginAction("BisectGlobalX", "Global X", "", nil, nil)
	local bisectGlobalY = common:CreatePluginAction("BisectGlobalY", "Global Y", "", nil, nil)
	local bisectGlobalZ = common:CreatePluginAction("BisectGlobalZ", "Global Z", "", nil, nil)

	bisectGlobalX.Triggered:Connect(CreateBisector(Enum.Axis.X, true))
	bisectGlobalY.Triggered:Connect(CreateBisector(Enum.Axis.Y, true))
	bisectGlobalZ.Triggered:Connect(CreateBisector(Enum.Axis.Z, true))

	pluginMenu = common:CreatePluginMenu("PartBisector", "Part Bisector", "rbxassetid://88889481728921", "rbxassetid://81720630505836")
	pluginMenu:AddAction(bisectLocalX)
