					<ProtectedString name="Source"><![CDATA[local selection = game:GetService("Selection")
local common = require(script.Parent.Parent.ToolsCommon)

local BisectError = {
	TryBisectSphere 	 = 0,
	TryBisectCornerWedge = 1,
	BadWedgeBisect 		 = 2,
	BadCylinderBisect	 = 3,
	BadGlobalBisect		 = 4,
	BadSizeBisect		 = 5,
	BisectTooSmall		 = 6,
	InvalidInstance		 = 7,
}

local function TryBisect(part: Instance, axis: Enum.Axis, isGlobal: boolean, snap: number)
	local isWeirdPart = part:IsA("WedgePart") or part:IsA("CornerWedgePart")
	local isValidPart = part:IsA("Part") or isWeirdPart
	if not isValidPart then warn(`Can't bisect part of type {part.ClassName}!`) return end
	
	part = part :: Part
	
	local pType: Enum.PartType
	if isWeirdPart then
		pType = part:IsA("WedgePart") and Enum.PartType.Wedge or Enum.PartType.CornerWedge
	else
		pType = part.Shape
	end
	
	if pType == Enum.PartType.Ball then
		warn("Can't bisect Sphere!")
		return
	elseif pType == Enum.PartType.CornerWedge then
		warn("Can't bisect CornerWedge!")
		return
	end
	
	local sizeAxis: Vector3
	local posAxis: Vector3
	local size = part.Size
	local bisectPos = part.Position
	
	if not isGlobal then
		sizeAxis = Vector3.FromAxis(axis)
		posAxis = part.CFrame:VectorToWorldSpace(Vector3.FromAxis(axis))
	else
		sizeAxis = part.CFrame:VectorToObjectSpace(Vector3.FromAxis(axis))
		posAxis = Vector3.FromAxis(axis)
	end
	
	sizeAxis = Vector3.new( math.abs(sizeAxis.X), math.abs(sizeAxis.Y), math.abs(sizeAxis.Z) )
	--posAxis  = Vector3.new( math.abs(posAxis.X),  math.abs(posAxis.Y),  math.abs(posAxis.Z)  )
	
	if pType == Enum.PartType.Cylinder or pType == Enum.PartType.Wedge then
		if sizeAxis.X ~= 1 then 
			warn(`Parts of type {pType == Enum.PartType.Cylinder and "Cylinder" or "Wedge"} may only be bisected on their local X axis!`) 
			return 
		end
	end
	
	if isGlobal then 
		local orient = Vector3.new(part.CFrame:ToOrientation())
		orient = orient * (360/(math.pi*2))
		orient = orient * (Vector3.one-posAxis)
		
		local totalOrient = orient.X + orient.Y + orient.Z
		if not (((totalOrient % 90) > 89.999) or ((totalOrient % 90) < 0.001)) then
			print(totalOrient)
			print(totalOrient % 90)
			print(orient)
			warn("Can't globally bisect part not perpendicular to bisect axis!")
			return
		end
	end
	
	local bisectAxisSize = (size * sizeAxis).Magnitude
	local bisectAxisSnaps = bisectAxisSize / snap
	
	if bisectAxisSnaps < 2 then warn(`Cannot bisect part of size {bisectAxisSize}! No valid grid size!`) return end
	if math.round(bisectAxisSnaps) ~= bisectAxisSnaps then warn(`Cannot bisect part of size {bisectAxisSize}! Does not conform to grid!`) return end
	
	local sizeFac = math.ceil(bisectAxisSnaps / 2) / bisectAxisSnaps
	local unbisectSize = size * (Vector3.one-sizeAxis)
	local bisectSize = size * sizeAxis
	
	print(`SizeAxis: {sizeAxis}, PosAxis: {posAxis} BisectAxisSize: {bisectAxisSize}, BisectAxisSnaps: {bisectAxisSnaps}, SizeFac: {sizeFac}, UBSize: {unbisectSize}, BSize: {bisectSize}`)
	
	local p1BisectSize = (bisectAxisSize * sizeFac)
	local p2BisectSize = (bisectAxisSize * (1-sizeFac))
	
	local p1Size = unbisectSize + (sizeAxis * p1BisectSize)
	local p2Size = unbisectSize + (sizeAxis * p2BisectSize)
	
	local complementPart = part:Clone()
	complementPart.Parent = part.Parent
	
	part.Size = p1Size
	complementPart.Size = p2Size
	
	part.Position 			= bisectPos + (posAxis * (p1BisectSize / 2))
	complementPart.Position = bisectPos - (posAxis * (p2BisectSize / 2))
	
	return { part, complementPart }
	
end

local function BisectGroup(axis: Enum.Axis, isGlobal: boolean)
	local recording = common:RecordChanges("Part Bisect")
	if recording == nil then return end
	
	local snap = common:GetGridSize()
	local output = {}
	
	for _, instance in ipairs(selection:Get()) do
		local outParts = TryBisect(instance, axis, isGlobal, snap)
		if outParts == nil then output[#output+1] = instance continue end
		
		for _, outV in ipairs(outParts) do
			output[#output+1] = outV
		end
	end
	
	selection:Set(output)
	common:CommitChanges(recording)
end

local function CreateBisector(axis: Enum.Axis, isGlobal: boolean)
	return function()
		BisectGroup(axis, isGlobal)
	end
end

